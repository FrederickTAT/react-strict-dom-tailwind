{
  "name": "eslint-plugin-react-strict-dom-tailwind",
  "version": "0.1.1-beta.0",
  "description": "ESLint plugin to validate react-strict-dom-tailwind tw() function usage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run extract-styles && tsc",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "extract-styles": "node scripts/extract-styles.js",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist",
    "lib",
    "types",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FrederickTAT/react-strict-dom-tailwind.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "react-strict-dom",
    "tailwind",
    "stylex"
  ],
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.11.0",
    "@typescript-eslint/utils": "^6.0.0",
    "eslint": "^9.6.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {}
}